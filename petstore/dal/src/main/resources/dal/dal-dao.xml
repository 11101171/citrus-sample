<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://localhost:8080/schema/www.springframework.org/schema/beans/spring-beans.xsd
    "
    default-autowire="byName">

    <bean id="userDao" class="com.alibaba.sample.petstore.dal.dao.ibatis.IbatisUserDao" />
    <bean id="categoryDao" class="com.alibaba.sample.petstore.dal.dao.ibatis.IbatisCategoryDao" />
    <bean id="productDao" class="com.alibaba.sample.petstore.dal.dao.ibatis.IbatisProductDao" />
    <bean id="productItemDao"
        class="com.alibaba.sample.petstore.dal.dao.ibatis.IbatisProductItemDao" />
    <bean id="sequenceDao" class="com.alibaba.sample.petstore.dal.dao.ibatis.IbatisSequenceDao" />

</beans>
